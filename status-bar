playPath: string;\\n  isGitRepo: boolean;\\n  gitStatus?: GitStatus;\\n}\\n\\n/**\\n * Safe command execution with error handling\\n */\\nfunction safeExec(command: string, options?: { silent?: boolean }): string | null {\\n  try {\\n    return execSync(command, { \\n      encoding: 'utf-8', \\n      stdio: options?.silent ? 'pipe' : ['pipe', 'pipe', 'ignore'] \\n    }).toString().trim();\\n  } catch {\\n    return null;\\n  }\\n}\\n\\n/**\\n * Check if current directory is a git repository\\n */\\nfunction isGitRepository(): boolean {\\n  return safeExec('git rev-parse --git-dir', { silent: true }) !== null;\\n}\\n\\n/**\\n * Get current git branch name\\n */\\nfunction getGitBranch(): string | undefined {\\n  const branch = safeExec('git branch --show-current');\\n  return branch || undefined;\\n}\\n\\n/**\\n * Parse git status porcelain output into counts\\n */\\nfunction parseGitStatus(): Omit<GitStatus, 'branch'> {\\n  const status = safeExec('git status --porcelain');\\n  if (!status) {\\n    return { modified: 0, deleted: 0, untracked: 0, staged: 0 };\\n  }\\n\\n  let modified = 0;\\n  let deleted = 0;\\n  let untracked = 0;\\n  let staged = 0;\\n\\n  const lines = status.split('\\\\n').filter(line => line.trim());\\n  \\n  for (const line of lines) {\\n    if (line.length < 2) continue;\\n    \\n    const indexStatus = line[0];\\n    const workTreeStatus = line[1];\\n    \\n    // Count staged changes (index status)\\n    if (indexStatus === 'A' || indexStatus === 'M' || indexStatus === 'D') {\\n      staged++;\\n    }\\n    \\n    // Count working tree changes\\n    if (workTreeStatus === 'M') {\\n      modified++;\\n    } else if (workTreeStatus === 'D') {\\n      deleted++;\\n    } else if (indexStatus === '?' && workTreeStatus === '?') {\\n      untracked++;\\n    }\\n  }\\n\\n  return { modified, deleted, untracked, staged };\\n}\\n\\n/**\\n * Format path for display (truncate if too long, replace home with ~)\\n */\\nfunction formatDisplayPath(fullPath: string, maxLength: number = 40): string {\\n  const homeDir = os.homedir();\\n  let displayPath = fullPath.startsWith(homeDir) \\n    ? '~' + fullPath.slice(homeDir.length)\\n    : fullPath;\\n\\n  if (displayPath.length <= maxLength) {\\n    return displayPath;\\n  }\\n\\n  // Truncate from the left, keeping the end\\n  return '...' + displayPath.slice(-(maxLength - 3));\\n}\\n\\n/**\\n * Get current project context\\n */\\nfunction getCurrentProjectContext(): ProjectContext {\\n  const cwd = process.cwd();\\n  const displayPath = formatDisplayPath(cwd);\\n  const isGitRepo = isGitRepository();\\n  \\n  let gitStatus: GitStatus | undefined;\\n  if (isGitRepo) {\\n    const branch = getGitBranch();\\n    const statusCounts = parseGitStatus();\\n    gitStatus = { branch, ...statusCounts };\\n  }\\n\\n  return {\\n    cwd,\\n    displayPath,\\n    isGitRepo,\\n    gitStatus,\\n  };\\n}\\n\\n/**\\n * Hook for project context with smart updating\\n */\\nexport function useProjectContext() {\\n  const [context, setContext] = useState<ProjectContext>(() => getCurrentProjectContext());\\n  const [isRefreshing, setIsRefreshing] = useState(false);\\n\\n  const refreshContext = useCallback(async () => {\\n    if (isRefreshing) return; // Prevent concurrent refreshes\\n    \\n    setIsRefreshing(true);\\n    try {\\n      // Small delay to avoid blocking UI during git operations\\n      await new Promise(resolve => setTimeout(resolve, 0));\\n      const newContext = getCurrentProjectContext();\\n      setContext(newContext);\\n    } finally {\\n      setIsRefreshing(false);\\n    }\\n  }, [isRefreshing]);\\n\\n  // Refresh on window focus/blur\\n  useEffect(() => {\\n    const handleFocus = () => refreshContext();\\n    const handleBlur = () => refreshContext();\\n\\n    // In a terminal app, we can't use window events, but we can use process events\\n    process.on('SIGCONT', handleFocus); // Terminal regains focus\\n    process.on('SIGTSTP', handleBlur);  // Terminal loses focus (Ctrl+Z)\\n\\n    // Initial refresh\\n    refreshContext();\\n\\n    return () => {\\n      process.off('SIGCONT', handleFocus);\\n      process.off('SIGTSTP', handleBlur);\\n    };\\n  }, [refreshContext]);\\n\\n  return {\\n    context,\\n    refreshContext,\\n    isRefreshing,\\n  };\\n}\"\n          }\n        }\n      ]\n    },\n    {\n      \"role\": \"user\",\n      \"content\": [\n        {\n          \"type\": \"tool_result\",\n          \"tool_use_id\": \"toolu_01HSqVjWDc647p3s1sNe5ThT\",\n          \"content\": \"Added task #3: Implement two-row status bar with project context (cwd + git info)\"\n        }\n      ]\n    },\n    {\n      \"role\": \"user\",\n      \"content\": [\n        {\n          \"type\": \"tool_result\",\n          \"tool_use_id\": \"toolu_01UccBWJwicGZCZHgtaYB4q3\",\n          \"content\": \"Tool execution denied by approval policy\",\n          \"is_error\": true\n        }\n      ]\n    },\n    {\n      \"role\": \"user\",\n      \"content\": \"sorry. please don't start implementation. please write out a spec as a series of prompts/tasks for an llm coding assistant. assume they have no project context, so give the a rundown of all the files they might need to touch, how to do the implementation, etc. test first. dry. yagni.  put it in docs/plans/double-status.md\"\n    },\n    {\n      \"role\": \"assistant\",\n      \"content\": [\n        {\n          \"type\": \"tool_use\",\n          \"id\": \"toolu_01PvYF2hCpL6ukqFgHHXfdbj\",\n          \"name\": \"file_write\",\n          \"input\": {\n            \"path\": \"docs/plans/double-status.md\",\n            \"content\": \"# Enhanced Status Bar with Project Context\\n\\n## Overview\\n\\nAdd a second row to the terminal status bar displaying current working directory and git repository information. This helps users understand their current project context when using Lace.\\n\\n## Current State\\n\\nThe status bar (`src/interfaces/terminal/components/status-bar.tsx`) currently shows:\\n```\\n[Provider:Model] â€¢ [ThreadID] â€¢ [Messages] â€¢ [Tokens] â€¢ [Status]\\n```\\n\\n## Target State\\n\\nTwo-row status bar:\\n```\\nRow 1: [Provider:Model] â€¢ [ThreadID] â€¢ [Messages] â€¢ [Tokens] â€¢ [Status]\\nRow 2: [/path/to/current/directory] â€¢ [branch-name] â€¢ [3 modified; 1 untracked]\\n```\\n\\n## Requirements\\n\\n- **Path display**: Show full path, truncated to ~40 characters from the right (e.g., `.../long/path/to/project`)\\n- **Home directory**: Replace home with `~` (e.g., `~/Documents/project`)\\n- **Git branch**: Show current branch name if in git repository\\n- **Working copy status**: Show counts like `3 modified; 6 deleted; 1 untracked; 2 staged` (only non-zero counts)\\n- **Non-git directories**: Show only path, no git info\\n- **Unicode characters**: Use traditional unicode (no emoji due to Ink measurement issues)\\n- **Update timing**: Only refresh on focus changes or command completion, not real-time\\n- **Performance**: Cache results, avoid blocking UI during git operations\\n\\n## Implementation Plan\\n\\n### Phase 1: Create Project Context Hook\\n\\n**File**: `src/interfaces/terminal/hooks/use-project-context.ts`\\n\\nCreate a React hook that:\\n1. Gets current working directory with `process.cwd()`\\n2. Formats path for display (truncate, replace home with `~`)\\n3. Checks if directory is git repo with `git rev-parse --git-dir`\\n4. Gets branch name with `git branch --show-current`\\n5. Parses `git status --porcelain` output into counts\\n6. Caches results and provides refresh function\\n7. Handles all git command failures gracefully\\n\\n**Interface**:\\n```typescript\\ninterface GitStatus {\\n  branch?: string;\\n  modified: number;\\n  deleted: number;\\n  untracked: number;\\n  staged: number;\\n}\\n\\ninterface ProjectContext {\\n  cwd: string;\\n  displayPath: string;\\n  isGitRepo: boolean;\\n  gitStatus?: GitStatus;\\n}\\n\\nfunction useProjectContext(): {\\n  context: ProjectContext;\\n  refreshContext: () => Promise<void>;\\n  isRefreshing: boolean;\\n}\\n```\\n\\n**Git Status Parsing Logic**:\\n- `git status --porcelain` format: `XY filename`\\n- X = index status, Y = working tree status\\n- Count `M` in Y position as modified\\n- Count `D` in Y position as deleted  \\n- Count `??` as untracked\\n- Count `A`, `M`, `D` in X position as staged\\n\\n**Tests**: Write comprehensive tests for:\\n- Path formatting (long paths, home directory replacement)\\n- Git status parsing (various combinations of file states)\\n- Non-git directory handling\\n- Command execution failures\\n\\n### Phase 2: Update Status Bar Component\\n\\n**File**: `src/interfaces/terminal/components/status-bar.tsx`\\n\\nModify StatusBar component to:\\n1. Add new props for project context\\n2. Render two rows instead of one\\n3. Format git status counts (only show non-zero counts)\\n4. Handle responsive layout for very narrow terminals\\n\\n**New Props**:\\n```typescript\\ninterface StatusBarProps {\\n  // ... existing props\\n  projectContext?: ProjectContext;\\n}\\n```\\n\\n**Display Logic**:\\n- Row 1: Keep existing content unchanged\\n- Row 2: `[displayPath] â€¢ [branch] â€¢ [status counts]`\\n- Use unicode characters: `â”ƒ` `â€¢` `Â±` `+` `-` `?` etc.\\n- Git status format: `3Â±` (modified), `2+` (staged), `1-` (deleted), `2?` (untracked)\\n- Only show non-zero counts, separated by spaces\\n\\n**Tests**: Update existing tests and add new ones for:\\n- Two-row rendering\\n- Git status formatting\\n- Non-git directory display\\n- Edge cases (very long paths, no git info)\\n\\n### Phase 3: Integrate with Terminal Interface\\n\\n**File**: `src/interfaces/terminal/terminal-interface.tsx`\\n\\n1. Import and use `useProjectContext` hook\\n2. Pass project context to StatusBar component\\n3. Trigger refresh on appropriate events:\\n   - After tool execution completes\\n   - On focus changes (if detectable)\\n   - Initial mount\\n\\n**Integration points**:\\n- Add hook call near other state management\\n- P