{
  "name": "lace-web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.18.3"
  },
  "scripts": {
    "dev": "tsx server.ts",
    "dev:debug": "LACE_LOG_LEVEL=debug LACE_LOG_STDERR=true tsx server.ts",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "next lint; npx tsc --noEmit",
    "lint:fix": "next lint --fix",
    "format": "prettier --write app components e2e hooks lib public scripts test-utils types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.0",
    "@fortawesome/free-solid-svg-icons": "6.7.0",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@heroicons/react": "2.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.15",
    "@types/highlight.js": "^9.12.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "5.0.46",
    "dompurify": "^3.2.6",
    "framer-motion": "12.23.3",
    "highlight.js": "^11.11.1",
    "next": "15.3.5",
    "open": "^10.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "zod": "^3.25.69"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.54.1",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/nextjs-vite": "^9.1.2",
    "@storybook/react": "^9.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "chromatic": "^6.0.0",
    "eslint-config-next": "^15.3.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-storybook": "^9.1.2",
    "jsdom": "^26.1.0",
    "msw": "^2.7.0",
    "postcss": "^8.4.38",
    "storybook": "^9.1.2",
    "tailwindcss": "4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  }
}
