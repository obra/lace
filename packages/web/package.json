{
  "name": "lace-web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "next build",
    "start": "next start -p 3005",
    "lint": "next lint",
    "dev:debug": "LACE_LOG_LEVEL=debug LACE_LOG_STDERR=true npm run dev",
    "dev:debug-file": "LACE_LOG_LEVEL=debug LACE_LOG_FILE=/tmp/lace-web.log npm run dev",
    "lint:fix": "next lint --fix",
    "format": "prettier --write",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run --exclude=\"**/*{e2e,integration}*\"",
    "test:integration": "vitest run \"integration\"",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.0",
    "@fortawesome/free-solid-svg-icons": "6.7.0",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@heroicons/react": "2.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.15",
    "@types/highlight.js": "^9.12.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "5.0.46",
    "dompurify": "^3.2.6",
    "framer-motion": "12.23.3",
    "highlight.js": "^11.11.1",
    "next": "15.3.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "^0.0.1",
    "zod": "^3.25.69"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.54.1",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/nextjs-vite": "^9.0.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "chromatic": "^6.0.0",
    "eslint-config-next": "^15.3.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "storybook": "^9.0.17",
    "tailwindcss": "4.1.11",
    "tsx": "^4.19.2",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  }
}
