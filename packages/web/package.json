{
  "name": "lace-web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.18"
  },
  "scripts": {
    "dev": "tsx server-custom.ts",
    "dev:debug": "LACE_LOG_LEVEL=debug LACE_LOG_STDERR=true tsx server-custom.ts",
    "build": "react-router build",
    "build:standalone": "react-router build",
    "start": "NODE_ENV=production tsx server-custom.ts",
    "lint": "NODE_ENV=development eslint; npx tsc --noEmit",
    "lint:fix": "eslint --fix",
    "format": "prettier --write app components e2e hooks lib public scripts test-utils types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "pretest:playwright": "npm run build",
    "pretest:playwright:ci": "npm run build",
    "pretest:playwright:ui": "npm run build",
    "test:playwright": "playwright test",
    "test:playwright:ci": "TEST_FILES=$(grep -v '^#' .playwright-ci-allowlist | grep -v '^$' | tr '\\n' ' ') && if [ -n \"$TEST_FILES\" ]; then playwright test $TEST_FILES; else echo 'No tests in allowlist, skipping Playwright tests'; fi",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:ui:dev": "playwright test --ui",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@fontsource-variable/dm-sans": "^5.2.6",
    "@fontsource/google-sans-code": "^5.2.1",
    "@fontsource/lato": "^5.2.6",
    "@fortawesome/fontawesome-svg-core": "6.7.0",
    "@fortawesome/free-solid-svg-icons": "6.7.0",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@heroicons/react": "2.2.0",
    "@lace/core": "file:../core",
    "@react-router/dev": "^7.8.2",
    "@react-router/express": "^7.8.2",
    "@react-router/node": "^7.8.2",
    "@react-router/serve": "^7.8.2",
    "@sentry/node": "^10.5.0",
    "@sentry/react": "^10.5.0",
    "@sentry/vite-plugin": "^4.1.1",
    "@types/express": "^5.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "daisyui": "^5.0.46",
    "dompurify": "^3.2.6",
    "express": "^4.21.2",
    "highlight.js": "^11.11.1",
    "isbot": "^5",
    "motion": "^12.23.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "vite": "^7.1.3",
    "zod": "^3.25.69"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@ladle/react": "^5.0.3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "chromatic": "^6.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.7.0",
    "playwright-msw": "^3.0.1",
    "postcss": "^8.4.38",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@swc/core",
    "@tailwindcss/oxide",
    "unrs-resolver"
  ]
}
