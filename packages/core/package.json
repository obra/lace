{
  "name": "@lace/core",
  "version": "0.0.1",
  "type": "module",
  "description": "Core backend logic for Lace AI coding assistant",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.18.3"
  },
  "scripts": {
    "build": "tsc && tsc-alias && cp -r src/config/prompts dist/config/ 2>/dev/null || true",
    "build:clean": "rm -rf dist && npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src --exclude \"**/*.integration.test.ts\" --exclude \"**/*.e2e.test.ts\"",
    "test:integration": "vitest run src --include \"**/*.integration.test.ts\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@lmstudio/sdk": "^1.2.1",
    "@typescript-eslint/utils": "^8.39.1",
    "better-sqlite3": "^11.10.0",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.2",
    "ollama": "^0.5.16",
    "openai": "^4.77.0",
    "turndown": "^7.2.0",
    "zod": "^3.25.69",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.16.0",
    "@types/turndown": "^5.0.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.6.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./agents/agent": {
      "types": "./dist/agents/agent.d.ts",
      "import": "./dist/agents/agent.js"
    },
